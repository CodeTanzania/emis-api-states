{
  "name": "@codetanzania/emis-api-states",
  "version": "0.10.3",
  "description": "EMIS Redux state management library",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf lib es umd",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --fix --ext .jsx,.js src/ test/",
    "build": "npm run clean && npm run lint && BABEL_ENV=production rollup -c",
    "commit": "git-cz",
    "commit:all": "git add -A && git-cz",
    "cmt": "git add -A && git-cz",
    "changelog": "changelog"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeTanzania/emis-api-states.git"
  },
  "keywords": [
    "codetanzania",
    "emis",
    "emis-states",
    "emis-api-states",
    "redux",
    "redux-starter-kit"
  ],
  "files": [
    "lib/",
    "es/",
    "src/",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "contributors": [
    {
      "name": "Benson Maruchu",
      "email": "benmaruchu@gmail.com",
      "url": "https://github.com/BenMaruchu"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CodeTanzania/emis-api-states/issues"
  },
  "homepage": "https://github.com/CodeTanzania/emis-api-states#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-react": "7.0.0",
    "@codetanzania/emis-api-client": "0.14.0",
    "@commitlint/cli": "8.0.0",
    "@commitlint/config-conventional": "8.0.0",
    "@commitlint/travis-cli": "8.0.0",
    "@testing-library/react": "8.0.1",
    "axios": "0.19.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "24.8.0",
    "commitizen": "3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jest": "22.6.4",
    "eslint-plugin-jsdoc": "7.2.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "faker": "4.1.0",
    "generate-changelog": "1.7.1",
    "husky": "2.4.0",
    "inflection": "1.12.0",
    "jest": "24.8.0",
    "lint-staged": "8.2.0",
    "lodash": "4.17.11",
    "moment": "2.24.0",
    "prettier": "1.18.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "7.0.3",
    "redux": "4.0.1",
    "redux-mock-store": "1.5.3",
    "redux-starter-kit": "0.5.1",
    "redux-thunk": "2.3.0",
    "rollup": "1.14.3",
    "rollup-plugin-babel": "4.3.2"
  },
  "peerDependencies": {
    "@codetanzania/emis-api-client": ">=0.13.2",
    "axios": ">=0.18.0",
    "inflection": ">=1.12.0",
    "lodash": ">=4.17.11",
    "prop-types": ">=15.7.2",
    "react": ">=16.8.6",
    "redux": ">=4.0.1",
    "react-redux": ">=7.0.2",
    "redux-starter-kit": ">=0.4.3"
  },
  "jest": {
    "verbose": false,
    "collectCoverageFrom": [
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/coverage/**",
      "**/src/**"
    ],
    "collectCoverage": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
