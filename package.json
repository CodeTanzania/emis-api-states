  "homepage": "https://github.com/CodeTanzania/emis-api-states#readme",
  "dependencies": {
    "@codetanzania/emis-api-client": "^0.1.0",
    "axios": "^0.18.0",
    "inflection": "^1.12.0",
    "lodash": "^4.17.11",
    "react": "^16.7.0",
    "react-redux": ">=6.0.0",
    "redux": ">=4.0.1",
    "redux-starter-kit": "^0.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
